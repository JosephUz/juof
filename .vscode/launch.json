{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Trial Example",
            "program": "${workspaceFolder}/examples/trial/app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Basic Example",
            "program": "${workspaceFolder}/examples/basic/app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Attribute Example",
            "program": "${workspaceFolder}/examples/attribute/app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Function Example",
            "program": "${workspaceFolder}/examples/function/app.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Mocha Current File",
            "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
            "args": [
                "--timeout",
                "999999",
                "--colors",
                "${file}"
            ],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}